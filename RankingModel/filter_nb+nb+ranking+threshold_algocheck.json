{
  "workflow": [
    {
      "model": "FilterModel",
      "limit_features": ["wikipedia_auto_suggest_has_link", "title_has_algorithm",
        "description_has_algorithm"],
      "use_pairwise_features": true,
      "use_nb_score": true,
      "pairwise_limit_features": ["title_fuzzy_ratio", "title_fuzzy_partial_ratio",
        "title_fuzzy_simhash", "title_tfidf", "iwlinks_fuzzy_ratio",
        "summary_similarity", "code_comments", "code_funcnames",
        "impl_commentary", "wikipedia_auto_suggest"],
      "num_neg": 1
    },
    {
      "model": "BinaryNBModel",
      "limit_features": ["title_fuzzy_ratio", "title_fuzzy_partial_ratio",
        "title_fuzzy_simhash", "title_tfidf", "iwlinks_fuzzy_ratio",
        "summary_similarity", "code_comments", "code_funcnames",
        "impl_commentary", "wikipedia_auto_suggest"],
      "num_neg": 1
    }, {
      "model": "RankingModel",
      "limit_features": ["title_fuzzy_ratio", "title_fuzzy_partial_ratio",
        "title_fuzzy_simhash", "title_tfidf", "iwlinks_fuzzy_ratio",
        "summary_similarity", "code_comments", "code_funcnames",
        "impl_commentary", "wikipedia_auto_suggest"],
      "num_neg": 1
    }, {
      "model": "ThresholdModel",
      "use_rank_score": true,
      "num_neg": 1,
      "limit_features": ["wikipedia_auto_suggest_has_link", "title_has_algorithm",
        "description_has_algorithm", "wikipage_category_algorithm_check",
        "wikipage_content_algorithm_check"]
    }
  ],
  "balanced_train": false,
  "balanced_test": false,
  "num_splits": 5
}
